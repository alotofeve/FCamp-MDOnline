LectureController:

postLecture:
/POST /postLecture
INPUT{
  title: string
  body: string
}
OUTPUT{
  isSaved: Boolean
}

getLectureByDoctor:
/GET /getLectureByDoctor
INPUT{
  doctorID: Int
}
OUTPUT{
  lectureList
}

getLectureByPatient:
/GET /getLectureByPatient
INPUT{
  patientID: Int
}
OUTPUT{
  lectureList
}

deleteLecture:
/POST /deleteLecture
INPUT{
  lectureID: Int
}
OUTPUT{
  isDeleted: Boolean
}
